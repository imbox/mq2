{
  "name": "mq2",
  "version": "3.0.0",
  "description": "Second generation ImBox RabbitMQ client",
  "homepage": "https://github.com/imbox/mq2#readme",
  "bugs": {
    "url": "https://github.com/imbox/mq2/issues"
  },
  "license": "",
  "author": "Mathias Lundell",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbox/mq2.git"
  },
  "private": true,
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "*.d.ts",
    "index.js",
    "lib",
    "types"
  ],
  "engines": {
    "node": ">=14.17.4"
  },
  "scripts": {
    "test": "tap test/*.test.js --no-coverage",
    "test:tap": "tap test/*.test.js --no-coverage ",
    "test:tdd": "tap test/*.test.js -w --no-coverage-report",
    "test:typescript": "tsd",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettierx --write '**/*.js'",
    "format:watch": "onchange '**/*.js' -- prettierx --write {{changed}}"
  },
  "devDependencies": {
    "@types/amqp-connection-manager": "^2.0.12",
    "@types/amqplib": "^0.8.1",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettierx": "0.18.0",
    "eslint-plugin-promise": "4.2.1",
    "onchange": "^7.1.0",
    "prettierx": "0.18.3",
    "proxyquire": "^2.1.3",
    "rewiremock": "^3.14.3",
    "tap": "^15.0.9",
    "tsd": "^0.17.0"
  },
  "dependencies": {
    "amqp-connection-manager": "luddd3/node-amqp-connection-manager#6826be2",
    "amqplib": "^0.8.0",
    "rabbit-routingkey-parser": "^1.1.4"
  },
  "tsd": {
    "directory": "test/types",
    "compilerOptions": {
      "esModuleInterop": true
    }
  }
}

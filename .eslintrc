{
  "root": true,
  "plugins": [
    "promise",
    "prettierx"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "extends": [
    "plugin:promise/recommended",
    "standard",
    "plugin:prettierx/standardx"
  ],
  "rules": {
    "prettierx/options": "error",
    "no-unused-vars": ["error", {"args": "all", "argsIgnorePattern": "^_"}]
  }
}
